{
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "env": {
    "node": true,
    "jquery": true
  },
  "rules": {
    "strict": [0, "global"],
    "indent": [2, 4],
    "space-before-function-paren": 0,
    "object-curly-spacing": 0,
    "comma-dangle": 0,
    "class-methods-use-this": 0
  }
}